{
  "name": "GOLD Additions",
  "description": "Quality of life implementations into UCSB's course registration system GOLD",
  "version": "1.2.0",
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "html/popup.html",
    "default_icon": "icons/icon128.png"
  },
  "icons": {
    "16": "icons/icon16.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://my.sa.ucsb.edu/gold/ResultsFindCourses.aspx"
      ],
      "js": [
        "scripts/utils.js",
        "scripts/instructorLink.js"
      ]
    },
    {
      "matches": [
        "https://my.sa.ucsb.edu/gold/StudentSchedule.aspx"
      ],
      "js": [
        "scripts/utils.js",
        "scripts/exportSchedule.js",
        "scripts/instructorLink.js"
      ]
    },
    {
      "matches": [
        "https://my.sa.ucsb.edu/gold/RegistrationInfo.aspx"
      ],
      "js": [
        "scripts/save_quarter_info.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "html/ics-settings.html",
        "css/ics-settings.css"
      ],
      "matches": [
        "https://my.sa.ucsb.edu/*"
      ]
    }
  ],
  "manifest_version": 3
}